        <script><![CDATA[(function executeRule(current, previous) {

    if (!current.request_asset || !current.quantity) {
        current.total_cost = 0;
        return;
    }

    var assetGR = new GlideRecord('x_1862743_asset_catalog');
    if (assetGR.get(current.request_asset)) {

        var unitCost = parseFloat(assetGR.unit_cost) || 0;
        var quantity = parseInt(current.quantity, 10) || 0;

        current.total_cost = unitCost * quantity;
    } else {
        current.total_cost = 0;
    }

})(current, previous);
]]></script>
