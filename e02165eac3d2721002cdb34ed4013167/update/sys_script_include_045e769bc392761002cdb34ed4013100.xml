        <script><![CDATA[var AssetCostAjax = Class.create();
AssetCostAjax.prototype = Object.extendsObject(AbstractAjaxProcessor, {

    getUnitCost: function() {
        var assetSysId = this.getParameter('sysparm_asset');

        if (!assetSysId)
            return '0';

        var gr = new GlideRecord('x_1862743_asset_catalog');
        if (gr.get(assetSysId)) {
            return gr.unit_cost.toString();
        }
        return '0';
    }

});
]]></script>
