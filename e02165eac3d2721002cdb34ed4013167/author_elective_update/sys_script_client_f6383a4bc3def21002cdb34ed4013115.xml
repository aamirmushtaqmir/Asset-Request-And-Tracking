        <messages/>
        <name>Calculate Cost on Asset Change</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
    if (isLoading || newValue === '') {
        return;
    }

    // Get the reference record for the selected asset
    g_form.getReference('request_asset', function(catalogItem) {
        var unitCost = catalogItem.unitCostt;
        var quantity = g_form.getValue('quantity');

        if (quantity && unitCost) {
            var total = parseFloat(unitCost) * parseInt(quantity);
            g_form.setValue('total_cost', total);
        }
    });
}]]></script>
