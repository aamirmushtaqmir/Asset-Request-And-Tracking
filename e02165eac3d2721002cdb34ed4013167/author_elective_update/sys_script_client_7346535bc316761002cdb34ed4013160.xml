        <messages/>
        <name>Real-time Total Cost Calculation  quanti</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
   
   alert("kfd");
    if (isLoading || newValue === '' || newValue <= 0) {
        return;
    }

    // 1. Get the Asset Reference to find the Unit Cost
    g_form.getReference('request_asset', function(asset) {
        var unitPrice = parseFloat(asset.unit_cost);
        var qty = parseInt(newValue);

        if (!isNaN(unitPrice)) {
            // 2. Perform the math: Total = Unit Cost * Quantity
            var total = unitPrice * qty;
            
            // 3. Update the Total Cost field instantly on the screen
            g_form.setValue('total_cost', total);
        }
    });
}]]></script>
